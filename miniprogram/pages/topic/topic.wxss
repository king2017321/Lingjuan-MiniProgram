/*/info/info.wxss */
.show{
  display: block !important;
  transition: all .2s ease 0s;
}
.hide{
  display: none !important;
  transition: all .2s ease 0s;
}
/**header**/
.row-s .col-10-4,.row-s .col-12-6,.row-s .col-12-8,.row-s .col-12-4{
	float: left;
}

.text-right {
	text-align: right;
}

.col-888 {
	color: #888;
}

.col-mar {
	margin: 0 1rem;
}

.row-s:after {
	content: "";
	display: block;
	height: 0;
	overflow: hidden;
	clear: both;
}

.row-s .col-12-6 {
	width: 50%;
}
.row-s .col-12-8 {
    width: 66.66666667%;
}
.row-s .col-12-4 {
    width: 33.33333333%;
}
.text-center {
	text-align: center;
}

.header_pr,
.header_pr>.search_term {
	background: #fff;
	width: 100%;
	top: 0;
	zoom: 1;
	left: 0;
}

.header_pr {
	-webkit-transform: translateY(0) translateX(0);
	transform: translateY(0) translateX(0);
	-webkit-transform: 0;
	-moz-transform: 0;
	-ms-transform: 0;
	transform: 0;
	position: fixed;
	z-index: 160;
	border-bottom: solid 1px #ddd
}

.icon_header>text.icon-zuojiantou view.search {
	margin: 0 10px 0 45px;
	overflow: initial
}

.icon_header,
.icon_header>text {
	z-index: 5;
	height: 45px;
	zoom: 1
}

.icon_header {
	width: 100%;
	line-height: 45px;
	position: relative
}

.icon_header>text{
	position: absolute;
	top: 0;
	width: 45px;
	text-align: center;
	color: #666;
	font-size: 1.29rem
}

.icon_header>text.icon-logo view.search {
	margin: 0 10px 0 85px
}

.icon_header>text.icon-zuojiantou view.search {
	margin: 0 10px 0 45px;
	overflow: initial
}

.icon_header>text.iconfont {
	font-size: 1.4rem
}

.header_goods {
	background: 0 0;
	border-bottom: 0
}

.header_goods .icon_header>text {
	display: inline-block;
	font-size: 18px;
	height: 30px;
	color: #fff;
	width: 30px;
	line-height: 30px;
	background: rgba(0, 0, 0, .3);
	border-radius: 100px;
	top: 7px;
	left: 7px
}

.header_goods .icon_header>text:nth-child(3) {
	right: 7px;
	left: auto
}

.header_goods .icon_header>view.title {
	overflow: inherit;
	padding: 0 60px;
	position: relative;
	z-index: 1;
	zoom: 1;
	top: 0;
	opacity: 0;
	background: #fff
}

.header_goods .icon_header>view.title text {
	font-size: 1rem;
	color: #666;
	position: relative;
	z-index: 1;
	zoom: 1
}

.header_goods .icon_header>view.title text:before {
	content: "";
	transition: all .4s ease 0s;
	-o-transition: all .4s ease 0s;
	-moz-transition: all .4s ease 0s;
	-webkit-transition: all .4s ease 0s;
	transform-origin: center;
	height: 3px;
	bottom: 0;
	width: 0;
	left: 50%;
	opacity: 0;
	margin-left: 0;
  bottom:0;
	position: absolute;
	z-index: 1;
	zoom: 1;
	display: block;
	overflow: hidden;
	background: #FC3F78;
	border-radius: 10px
}

.header_goods .icon_header>view.title text.active {
	color: #333;
	font-size: 1.1rem
}

.header_goods .icon_header>view.title text.active:before {
	width: 3rem;
	margin-left: -1.5rem;
	opacity: 1
}

.header_goods.active {
	background: #fff;
	border-bottom: 0
}

.header_goods.active .icon_header>view.title {
	top: 0;
	opacity: 1
}

.header_goods.active .icon_header>text:before {
	background: 0 0;
	color: #333
}

/**maximg**/
.goods_swiper{
  width: 100%;position: relative;
}
.goods_swiper swiper{
  height: 750rpx;
}
.goods_swiper image{
 width: 100%;
}
.goods_swiper .wx-swiper{
  background-color: #000;
}
.goods_swiper .dots{
  width: 100%;
  display: flex;
  justify-content: center;
  position: absolute;
  left: 0;
  bottom: 10rpx;
}
.goods_swiper .dots .dot{
  background: #ccc;
	opacity: 1;
	height: 12rpx;
	width: 12rpx;
	border-radius: 12rpx;
	-moz-border-radius: 12rpx;
	-webkit-border-radius: 12rpx;
	-o-border-radius: 12rpx;
	-ms-border-radius: 12rpx;
	transition: all .4s ease 0s;
	-o-transition: all .4s ease 0s;
	-moz-transition: all .4s ease 0s;
	-webkit-transition: all .4s ease 0s;
	transform-origin: center;
  margin: 0 4rpx;
}
.goods_swiper .dots .dot.active{
  background: #FC3F78;
	width: 28rpx;
}
