/**领券 淘口令**/
.layui-layer-shade {
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    position: fixed;
    pointer-events: auto;
}
.layui-layer{
  width: 85%;
  position: fixed;
  pointer-events: auto;
  top: 300rpx;
  left: 50%;
  margin: 0;
  padding: 0;
  background-color: #fff;
  box-shadow: 2rpx 2rpx 100rpx rgba(0,0,0,.3);
  -webkit-animation-fill-mode: both;
  animation-fill-mode: both;
  -webkit-animation-duration: .3s;
  animation-duration: .3s;
  border-radius: 16rpx;
  background: #fff;
}
.layui-layer .layui-layer-content{
  position: relative;
  overflow: inherit;
}
