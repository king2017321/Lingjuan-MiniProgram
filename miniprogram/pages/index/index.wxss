/**index.wxss**/
/**top**/
.search-wrap{
  height: 50rpx;
  padding: 20rpx;
  background-color: #fff;
  position: relative;
  border-bottom: 1rpx solid #ddd;
}
.search-wrap .search-logo{
  float: left;
  width: 117rpx;
  height: 50rpx;
  margin-right: 26rpx;
}
.search-wrap .search{
  float: left;
  width: 420rpx;
  height: 50rpx;
  border-radius:100rpx;
  text-align: center;
  background-color: #efeded;
  overflow: hidden;
}
.search-wrap .search .search-input-text {
   display: inline-block;
   vertical-align:top;
    background-color: #efeded;
    width: 292rpx;
    height: 50rpx;
    font-size: 24rpx;
    color: #c1c1c1;
    border: 0;
}
.search-wrap .search .search-icon {
    display: inline-block;
    vertical-align:top;
    width: 26rpx;
    height: 26rpx;
    padding:10rpx 14rpx;
    margin-top: 6rpx;
    border: 0;
    background-color: #efeded;
}
.search-wrap .toolbar-category {
    float: left;
    width: 40rpx;
    height: 40rpx;
    margin: 6rpx 0 0 60rpx;
}
.search-wrap .toolbar-category-hover{
  background-color: #fff;
}
.search-wrap .toolbar-category image{
  width: 100%;
  height: 100%;
} 
/**banner**/
.bannenr_box{
  width: 100%;
  height: auto;
}
.bannenr_box swiper{
  height: 288rpx;
}
.bannenr_box swiper .bannenr-img{
  width: 100%;
  height: 288rpx;
}
/**page-nav-box**/
.page-nav-box{
  width: 100%;
  height: 236rpx;
}
.page-nav-box .page-nav-swiper{
  height: 180rpx;
  padding: 30rpx 0 0;
white-space: nowrap;
}
.page-nav-box .page-nav-swiper .page-nav-item{
  display: inline-block;
  vertical-align: top;
  width: 150rpx !important;
  text-align: center;
  white-space:normal;
}
.page-nav-box .page-nav-swiper .page-nav-item image{
  width: 110rpx;
  height: 110rpx;
  vertical-align: top;
} 
.page-nav-box .page-nav-swiper .page-nav-item span{
  display: inline-block;
  vertical-align: top;
  width: 100%;
  text-align: center;
  font-size: 24rpx;
  color: #333;
  margin-top: 14rpx;
} 
.page-nav-box .page-nav-swiper .page-navt-itemho span{
  color: #ff4545;
}
.page-nav-box .t1-bottom{
  width: 100%;
  height: 24rpx;
  position: relative;
}
.page-nav-box .t1-bottom .pagination{
  position: absolute;
  top: 6rpx;
  left: 50%;
  width: 140rpx;
  height: 4rpx;
  margin-left: -70rpx;
  background: #F0F0F0;
  border-radius: 4rpx;
}
.page-nav-box .swiper-pagination-progressbar-fill {
  background: #ff4545;
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  -webkit-transform: scale(0);
  -ms-transform: scale(0);
  transform: scale(0);
  -webkit-transform-origin: left top;
  -ms-transform-origin: left top;
  transform-origin: left top;
}

/**大家都在领**/
/*title*/
.home_h3{
  padding: 20rpx 0;
  font-size:24rpx;
  font-weight: 400;
  display: block;
}
.col-mar{
  margin: 0 24rpx;
}
.home_h3 .text-left {
  font-size: 32rpx;
  line-height: 46rpx;
  float: left;
}
.home_h3 .text-right {
  float: left;
  position: relative;
  z-index: 1;
  zoom: 1;
  color: #aaa;
  font-size: 24rpx;
  line-height:46rpx;
  text-align: right;
}
.col-12-4 {
  width: 33.33333333%;
}
.col-12-8 {
  width: 66.66666667%;
}
.home_h3 .text-right text{
  color: #FC4D52;
}
.home_h3 .text-left:before {
  content: "";
  float: left;
  top: 3rpx;
  position: relative;
  z-index: 1;
  zoom: 1;
  width: 6rpx;
  height: 40rpx;
  border-radius: 6rpx;
  -moz-border-radius: 6rpx;
  -webkit-border-radius: 6rpx;
  -o-border-radius: 6rpx;
  -ms-border-radius: 6rpx;
  background: -moz-linear-gradient(top, #ff5d06 0, #fc3f78 100%);
  background: -webkit-gradient(linear, top top, top right, color-stop(0, #ff5d06), color-stop(100%, #fc3f78));
  background: -webkit-linear-gradient(top, #ff5d06 0, #fc3f78 100%);
  background: -o-linear-gradient(top, #ff5d06 0, #fc3f78 100%);
  background: -ms-linear-gradient(top, #ff5d06 0, #fc3f78 100%);
  background: linear-gradient(to top, #ff5d06 0, #fc3f78 100%);
  margin-right: 24rpx;
}
.home_ant_voucher .scroll-container{
  width: auto;
  padding:16rpx 0 20rpx;
  white-space: nowrap;
}
.scroll-container .scroll-item{
  display: inline-block;
  vertical-align:top;
  flex-shrink: 0;
  width:278rpx;
  margin-right: 10rpx;
  margin-top: 8rpx;
}
.scroll-container .scroll-item:first-child{
  margin-left: 24rpx;
}
.scroll-container .scroll-item .cent{
  padding: 20rpx 20rpx 0;
  border-radius: 16rpx 16rpx 0 0;
  box-shadow: 0 2rpx 20rpx 0 rgba(0, 0, 0, .1);
  position: relative;
  z-index: 1;
  zoom: 1;
}
.scroll-container .scroll-item .cent .img{
  display: block;
  position: relative;
  zoom: 1;
  z-index: 1;
  border-radius: 10rpx;
}
.scroll-container .scroll-item .cent .img image{
  display: block;
  width: 238rpx;
  height: 238rpx;
  border-radius: 10rpx;
}
.scroll-container .scroll-item .cent .img .text{
 display: block;
  position: absolute;
  zoom: 1;
  z-index: 10;
  bottom: 0;
  left: -8rpx;
  width: 100%;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  border-radius: 10rpx;
  -moz-border-radius: 10rpx;
  -webkit-border-radius: 10rpx;
  -o-border-radius: 10rpx;
  -ms-border-radius: 10rpx;
  background: -moz-linear-gradient(left, rgba(255, 113, 233, .91) 0, rgba(254, 92, 158, .91) 100%);
  background: -webkit-gradient(linear, left top, left right, color-stop(0, rgba(255, 113, 233, .91)), color-stop(100%, rgba(254, 92, 158, .91)));
  background: -webkit-linear-gradient(left, rgba(255, 113, 233, .91) 0, rgba(254, 92, 158, .91) 100%);
  background: -o-linear-gradient(left, rgba(255, 113, 233, .91) 0, rgba(254, 92, 158, .91) 100%);
  background: -ms-linear-gradient(left, rgba(255, 113, 233, .91) 0, rgba(254, 92, 158, .91) 100%);
  background: linear-gradient(to left, rgba(255, 113, 233, .91) 0, rgba(254, 92, 158, .91) 100%);
  color: #fff;
  padding: 4rpx 8rpx;
  font-size:24rpx;
  text-align: center;
}
.scroll-container .scroll-item .cent .name{
  display: block;
  line-height:56rpx;
  font-size: 28rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.scroll-container .scroll-item .cent .money{
  color: #FC4D52;
  font-size: 28rpx;
  padding-bottom: 20rpx;
}
.scroll-container .scroll-item .cent .money i{
  font-style: normal;
  font-size: 24rpx;
  margin-right: 6rpx;
}
.scroll-container .scroll-item .cent .money del{
  color: #bbb;
  font-size: 25rpx;
  text-decoration:line-through;
}
.scroll-container .scroll-item .cent .progress{
  position: absolute;
  z-index: 1;
  zoom: 1;
  left: 0;
  bottom: 0;
  overflow: hidden;
  height: 6rpx;
  background-color: #FFD6EC;
  width: 100%;
}
.scroll-container .scroll-item .cent .progress i {
  display: block;
  height: 6rpx;
  overflow: hidden;
  background-color: #FF66B7;
  width: 10rpx;
  transition: all 1s ease 0s;
  -o-transition: all 1s ease 0s;
  -moz-transition: all 1s ease 0s;
  -webkit-transition: all 1s ease 0s;
  transform-origin: center;
}

/*宝贝列表*/
.new-content {
    margin-bottom: 120rpx;
}
.new-content .new-title{
  height: 80rpx;
  background: #fff;
  width: 100%;
  text-align: center;
  position: relative;
  border-bottom: 1rpx #f5f5f5 solid;
}
.new-content .new-title image{
  width: 30%;
  margin-top: 20rpx;
}
.new-list_ .new-list-block{
    width: 100%;
    background: #fff;
    margin-top: 30rpx;
}
.new-list-block image{
  float: left;
  width: 37%;
  border-radius: 6rpx;
  margin-left: 10rpx;
  max-height:278rpx;
}
.new-list-block .new-list_right{
  float: right;
  width: 56%;
  color: #666;
  border-bottom: 1px #f5f5f5 solid;
  height: auto;
  margin-right: 2%;
}
.new-list-block .new-list_right .new-name{
    font-size: 28rpx;
    line-height: 1.5em;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
    overflow: hidden;
}
.new-list-block .new-list_right .new-quan{
    height: 50rpx;
    padding-right: 2.5rpx;
    line-height: 50rpx;
    color: rgba(253, 56, 93, 1);
    font-size: 28rpx;
    margin: 14rpx 0;
}
.new-list-block .new-list_right .new-quan text{
    background: rgba(253, 56, 93, 1);
    width: 50rpx;
    height: 50rpx;
    display: block;
    text-align: center;
    color: #fff;
    line-height: 50rpx;
    float: left;
    border-top-left-radius: 6rpx;
    border-bottom-left-radius: 6rpx;
}
.new-list-block .new-list_right .new-quan view{
    float: left;
    border: 1px rgba(253, 56, 93, 1) solid;
    line-height: 46rpx;
    text-align: center;
    padding: 0 6rpx;
    border-top-right-radius: 6rpx;
    border-bottom-right-radius: 6rpx;
}
.new-list-block .new-list_right .new-price {
    color: rgba(253, 56, 93, 1);
    font-size: 28rpx;
    width: 50%;
    float: left;
}
.new-list-block .new-list_right .new-sale{
    color: #999999;
    font-size: 24rpx;
    width: 50%;
    float: right;
    text-align: right;
}
.new-list-block .new-list_right .qiang_ {
    width: 100%;
    background: #ff5272;
    height: 70rpx;
    border: 0;
    border-radius: 10rpx;
    font-size: 28rpx;
    color: #fff;
    margin-top: 12rpx;
}
.new-list-block .new-list_right .qiang_active{
  background: #fd7a92;
}
/**底部导航**/
.ui_bot_tab{
  border-top: solid 1px #eee;
  position: fixed;
  zoom: 1;
  z-index: 110;
  left: 0;
  bottom: 0;
  width: 100%;
  background: #fff;
}
.ui_bot_tab .col-10-2 {
  width: 20%;
  float: left;
}
.ui_bot_tab .list {
    color: #666;
    text-align: center;
    font-size: 24rpx;
    padding: 6rpx 0 4rpx;
    position: relative;
    z-index: 1;
    zoom: 1;
    line-height: 40rpx;
}
.ui_bot_tab .list image {
    width: 48rpx;
    height: 48rpx;
    display: block;
    margin: auto;
}